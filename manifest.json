{
    "name": "JIM - Jobcan IMproved",
    "version": "1.1.0",
    "manifest_version": 2,
    "description": "ジョブカンの痒いところに手を伸ばすための拡張機能です。",
    "content_scripts": [
        {
            "": "外部ライブラリ",
            "matches": ["https://ssl.jobcan.jp/*"],
            "js": [
                "js/lib/jquery-3.6.0.min.js",
                "js/lib/moment-with-locales.min.js"
            ],
            "run_at": "document_start"
        },
        {
            "": "初期化",
            "matches": ["https://ssl.jobcan.jp/*"],
            "js": [
                "js/init.js"
            ],
            "run_at": "document_end"
        },

        {
            "": "全体共通",
            "matches": ["https://ssl.jobcan.jp/*"],
            "js": [
                "js/common.js"
            ]
        },

        {
            "": "employee 出勤簿",
            "matches": [
                "https://ssl.jobcan.jp/employee/attendance",
                "https://ssl.jobcan.jp/employee/attendance?*",
                "https://ssl.jobcan.jp/employee/attendance/",
                "https://ssl.jobcan.jp/employee/attendance/?*"
            ],
            "js": ["js/employee/attendance.js"]
        },
        {
            "": "employee 打刻修正",
            "matches": [
                "https://ssl.jobcan.jp/employee/adit/modify",
                "https://ssl.jobcan.jp/employee/adit/modify?*",
                "https://ssl.jobcan.jp/employee/adit/modify/",
                "https://ssl.jobcan.jp/employee/adit/modify/?*"
            ],
            "js": ["js/employee/adit_modify.js"]
        },
        {
            "": "employee シフト申請",
            "matches": [
                "https://ssl.jobcan.jp/employee/shift-pattern-request",
                "https://ssl.jobcan.jp/employee/shift-pattern-request?*",
                "https://ssl.jobcan.jp/employee/shift-pattern-request/",
                "https://ssl.jobcan.jp/employee/shift-pattern-request/?*"
            ],
            "js": ["js/employee/shift_pattern_request.js"]
        },
        {
            "": "employee 休暇申請一覧",
            "matches": [
                "https://ssl.jobcan.jp/employee/holiday",
                "https://ssl.jobcan.jp/employee/holiday?*",
                "https://ssl.jobcan.jp/employee/holiday/",
                "https://ssl.jobcan.jp/employee/holiday/?*"
            ],
            "js": ["js/employee/holiday.js"]
        },
        {
            "": "employee 新規休暇申請",
            "matches": [
                "https://ssl.jobcan.jp/employee/holiday/new",
                "https://ssl.jobcan.jp/employee/holiday/new?*",
                "https://ssl.jobcan.jp/employee/holiday/new/",
                "https://ssl.jobcan.jp/employee/holiday/new/?*"
            ],
            "js": ["js/employee/holiday_new.js"]
        },
        {
            "": "employee 残業申請一覧",
            "matches": [
                "https://ssl.jobcan.jp/employee/over-work",
                "https://ssl.jobcan.jp/employee/over-work?*",
                "https://ssl.jobcan.jp/employee/over-work/",
                "https://ssl.jobcan.jp/employee/over-work/?*"
            ],
            "js": ["js/employee/over_work.js"]
        },
        {
            "": "employee 新規残業申請",
            "matches": [
                "https://ssl.jobcan.jp/employee/over-work/new",
                "https://ssl.jobcan.jp/employee/over-work/new?*",
                "https://ssl.jobcan.jp/employee/over-work/new/",
                "https://ssl.jobcan.jp/employee/over-work/new/?*"
            ],
            "js": ["js/employee/over_work_new.js"]
        },
        {
            "": "employee 休日出勤申請一覧",
            "matches": [
                "https://ssl.jobcan.jp/employee/holidayworking/",
                "https://ssl.jobcan.jp/employee/holidayworking?*",
                "https://ssl.jobcan.jp/employee/holidayworking/",
                "https://ssl.jobcan.jp/employee/holidayworking/?*"
            ],
            "js": ["js/employee/holidayworking.js"]
        },
        {
            "": "employee 新規休日出勤申請",
            "matches": [
                "https://ssl.jobcan.jp/employee/holidayworking/new",
                "https://ssl.jobcan.jp/employee/holidayworking/new?*",
                "https://ssl.jobcan.jp/employee/holidayworking/new/",
                "https://ssl.jobcan.jp/employee/holidayworking/new/?*"
            ],
            "js": ["js/employee/holidayworking_new.js"]
        },
        {
            "": "employee スタッフ設定",
            "matches": [
                "https://ssl.jobcan.jp/employee/edit-info",
                "https://ssl.jobcan.jp/employee/edit-info?*",
                "https://ssl.jobcan.jp/employee/edit-info/",
                "https://ssl.jobcan.jp/employee/edit-info/?*"
            ],
            "js": ["js/employee/edit_info.js"]
        },
        {
            "": "employee 共通",
            "matches": ["https://ssl.jobcan.jp/employee/*"],
            "js": [
                "js/employee/common.js"
            ],
            "css": [
                "css/employee/style.css"
            ]
        }
    ]
}